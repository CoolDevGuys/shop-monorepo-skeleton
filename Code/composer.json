{
  "name": "cooldevguys/shop-monorepo-skeleton",
  "license": "MIT",
  "type": "project",
  "description": "A cool shop php application template",
  "require": {
    "php": "^8.0",
    "ext-amqp": "*",
    "ext-apcu": "*",
    "ext-json": "*",
    "ext-zend-opcache": "*",
    "ext-pdo": "*",
    "symfony/framework-bundle": "^5.3",
    "symfony/messenger": "^5.3",
    "symfony/dotenv": "^5.3",
    "symfony/yaml": "^5.3",
    "symfony/validator": "^5.3",
    "lambdish/phunctional": "^2.0",
    "ramsey/uuid": "^3.9",
    "doctrine/dbal": "^2.10",
    "doctrine/orm": "^2.7",
    "ocramius/proxy-manager": "2.8.1",
    "elasticsearch/elasticsearch": "^7.3",
    "monolog/monolog": "^2.1",
    "promphp/prometheus_client_php": "^2.0"
  },
  "require-dev": {
    "ext-xdebug": "*",
    "roave/security-advisories": "dev-latest",
    "behat/behat": "^3.6",
    "friends-of-behat/mink-extension": "^2.4",
    "behat/mink-browserkit-driver": "^1.3",
    "friends-of-behat/symfony-extension": "^2.1",
    "phpunit/phpunit": "^9.2",
    "mockery/mockery": "^1.4",
    "fakerphp/faker": "^1.10",
    "symfony/error-handler": "^5.3"
  },
  "autoload": {
    "psr-4": {
      "CoolDevGuys\\Applications\\Shop\\": "applications/shop/entrypoints",
      "CoolDevGuys\\Applications\\Dashboard\\": "applications/dashboard/entrypoints",
      "CoolDevGuys\\": [
        "src"
      ]
    }
  },
  "autoload-dev": {
    "psr-4": {
      "CoolDevGuys\\Tests\\": [
        "tests"
      ]
    }
  },
  "minimum-stability": "stable"
}
